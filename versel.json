{
  "version": 2,
  "env": {
    "MONGO_URI": "MONGODB_URI"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "output": "dist"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST"
      ]
    }
  ]
}